<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="700.0" styleClass="background" stylesheets="@JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationV2.Search1Controller">
	<children>
		<Label layoutX="267.0" layoutY="65.0" styleClass="header" stylesheets="@JMetroDarkTheme.css" text="Search ">
			<font>
				<Font size="50.0" />
			</font>
		</Label>
		<TextField fx:id="searchTerm" layoutX="140.0" layoutY="190.0" prefHeight="33.0" prefWidth="331.0" promptText="What are you looking for?" stylesheets="@JMetroDarkTheme.css">

			<font>
				<Font size="17.0" />
			</font>
		</TextField>
		<Button fx:id="Search1_goButton" layoutX="493.0" layoutY="190.0" mnemonicParsing="false" onAction="#search1_buttons" prefHeight="38.0" prefWidth="83.0" text="Go!" />
